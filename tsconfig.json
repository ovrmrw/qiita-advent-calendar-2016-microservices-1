{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "outDir": ".tscOutDir"
  },
  "exclude": [    
    "node_modules",
    "localhost",
    ".dest*"
  ]
}